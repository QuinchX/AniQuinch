*
{
	font-family: "Poppins",sans-serif;
	text-decoration: none;
	padding: 0%;
	margin: 0%;
}

body {
  background-color: #FFE5EC;
}

div.heading {
	font-size: 60px;
	text-align: center;
	color: white;
	text-shadow: 5px 5px #FFB3C6;
	padding-top: 100px;
}

div.gallery {
	margin: 20px;
	border: 8px solid #FFB3C6;
	float: left;
	width: 150px;
	padding: 50px;
	background-color: #FFC2D1;
}

div.gallery:hover {
  border: 8px solid #FE7F9C;
}

div.gallery img {
  width: 100%;
  height: auto;
}

div.desc {
	padding-top: 10px;
	padding-right: 0px;
	padding-bottom: 15px;
	padding-left: 0px;
	text-align: center;
	letter-spacing: 1px;
	font-size: 12px;
	color: black;
	font-weight: bold;
}

.button {
	background-color: #FF8fab;
	border: none;
	padding-top: 10px;
	padding-right: 0px;
	padding-bottom: 5px;
	padding-left: 0px;
	color: white;
	text-align: center;
	text-decoration: none;
	display: inline-block;
	font-size: 15px;
	cursor: pointer;
	position: absolute;
	width: 150px;
	height: 70px;
	font-family: sans-serif;
	font-weight: bold;
}

.button:hover {
	background-color: #ff7094;
}

#table {
  font-family: sans-serif;
  border-collapse: collapse;
  width: 100%;
  text-align: center;
  font-size: 15px;
}

#table td, #table th {
  border: 1px solid #FFB3C6;
  padding: 8px;

}

#table tr:nth-child(even){ 
	background-color: #FFD1DC;
	font-style: italic;
}

#table th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: center;
  background-color: #FF8fab;
  color: white;
  font-family: sans-serif;
}